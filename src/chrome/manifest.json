{
  "name": "Dualless",
  "version": "0.3.99.1",
  "description": "Dualless - For those who don't have dual monitor.",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "logo.png",
    "default_popup": "popup.html",
    "default_title" : "Dualless - Split / Merge windows"
  },
  "permissions": [
    "tabs"
  ],
  "background" : {
  	"scripts" : ["lib/jquery-1.7.1.min.js","lib/require.js","background.js"]
  },
  "options_page": "options.html"
}
