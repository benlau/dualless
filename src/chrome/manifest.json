{
  "name": "Dualless",
  "version": "0.4.1",
  "description": "Dualless - For those who don't have dual monitor.",
  "manifest_version": 3,
  "action": {
    "default_icon": "logo.png",
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "worker.js",
    "type": "module"
  },
  "permissions": [
    "tabs",
    "storage"
  ],
  "options_page": "options.html"
}
